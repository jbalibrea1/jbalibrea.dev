{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@*": ["./src/*"],
      "#*": ["./public/*"]
    }
  },
    "include": [
    "src/**/*",
    "public/**/*"
  ]
}
